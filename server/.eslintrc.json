{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 0,
    "consistent-return": 1,
    "implicit-arrow-linebreak": 0,
    "comma-dangle": 0,
    "indent": 0,
    "object-curly-spacing": 0,
    "no-multiple-empty-lines": 0,
    "arrow-parens": 0
  }
}
